                                    </Select>
                                </fb:control>
                            </fb:FilterGroupItem>
                            <fb:FilterGroupItem groupName="Standard" visibleInFilterBar="{store>/bColumns/2/filterVisible}" name="invoiceType" label="{i18n>InvcType}">
