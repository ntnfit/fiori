<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core">
	<VBox class="sapUiSmallMargin">
		<f:SimpleForm id="SimpleFormChangeColumn_threeGroups236"
			editable="true"
			layout="ColumnLayout"
			title="Supplier"
			columnsM="2"
			columnsL="3"
			columnsXL="{data>/column}"
			>
			<f:content>
				<core:Title text="Address" />
				<Label text="Name" />
				<Input id="name" value="{data>/SupplierName}" />
				<Label text="Street/No." />
				<Input value="{data>/Street}">
				</Input>
				<Input value="{data>/HouseNumber}">
					
				</Input>
				<Label text="ZIP Code/City" />
				<Input value="{data>/ZIPCode}">
					
				</Input>
				<Input value="{data>/City}" />
				<Label text="Country" />
				<Select id="country" selectedKey="{data>/Country}">
					<items>
						<core:Item text="England" key="England"/>
						<core:Item text="Germany" key="Germany"/>
						<core:Item text="USA" key="USA"/>
					</items>
				</Select>
				<Label text="Web" />
				<Input value="{data>/Url}" type="Url"/>
				<core:Title text="Contact" />
				<Label text="Twitter" />
				<Input value="{data>/Twitter}" />
				<Label text="Email" />
				<Input value="{data>/Email}" type="Email"/>
				<Label text="Tel." />
				<Input value="{data>/Tel}" type="Tel"/>
				<core:Title text="Other" />
				<Label text="SMS" />
				<Input value="{data>/Sms}" type="Tel"/>
				<Label text="Mobile" />
				<Input value="{data>/Mobile}" type="Tel"/>
				<Label text="Pager" />
				<Input value="{data>/Pager}" type="Tel"/>
				<Label text="Fax" />
				<Input value="{data>/Fax}" type="Tel"/>
			</f:content>
		</f:SimpleForm>
	</VBox>
</core:FragmentDefinition>