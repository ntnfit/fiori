                    </ui:Column>
                    <ui:Column autoResizable="true" minWidth="140px" visible="{store>/bColumns/30/visible}">
                        <Label text="{i18n>TaxClafNumber}" />
                        <ui:template>
                            <Text text="{store>taxClassificationNumber}" wrapping="false" />
