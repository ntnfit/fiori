                    </ui:Column>
                    <ui:Column autoResizable="true" minWidth="140px" visible="{store>/bColumns/29/visible}">
                        <Label text="{i18n>VersTaxClasNumber}" />
                        <ui:template>
                            <Text text="{store>taxClassificationNumberVersion}" wrapping="false" />
