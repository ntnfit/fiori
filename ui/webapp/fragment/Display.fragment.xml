<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core">
	<VBox class="sapUiSmallMargin">
		<f:SimpleForm id="SimpleFormDisplayColumn_threeGroups236"
			editable="false"
			layout="ColumnLayout"
			title="Supplier"
			columnsM="2"
			columnsL="3"
			columnsXL="{data>/column}"
			>
			<f:content>
				<core:Title text="Address" />
				<Label text="Name" />
				<Text id="nameText" text="{data>/SupplierName}" />
				<Label text="Street/No." />
				<Text text="{data>/Street} {data>/HouseNumber}" />
				<Label text="ZIP Code/City" />
				<Text text="{data>/ZIPCode} {data>/City}" />
				<Label text="Country" />
				<Text id="countryText" text="{data>/Country}" />
				<Label text="Web" />
				<Link text="Url" href="{data>/Url}" />
				<core:Title text="Contact" />
				<Label text="Twitter" />
				<Text text="{data>/Twitter}" />
				<Label text="Email" />
				<Text text="{data>/Email}" />
				<Label text="Tel." />
				<Text text="{data>/Tel}" />
				<core:Title text="Other" />
				<Label text="SMS" />
				<Text text="{data>/Sms}" />
				<Label text="Mobile" />
				<Text text="{data>/Mobile}" />
				<Label text="Pager" />
				<Text text="{data>/Pager}" />
				<Label text="Fax" />
				<Text text="{data>/Fax}" />
			</f:content>
		</f:SimpleForm>
	</VBox>
</core:FragmentDefinition>